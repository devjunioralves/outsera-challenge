    "dev": "nodemon --exec 'ts-node -r tsconfig-paths/register src/index.ts'",
    "lint": "eslint .",
    "lint:fix": "eslint 'src/**' --fix",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
