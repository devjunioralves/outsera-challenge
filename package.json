    "nodemon": "^3.1.4",
    "reflect-metadata": "^0.2.2",
    "sequelize": "^6.37.4",
    "ts-node": "^10.9.2",
    "tsyringe": "^4.8.0"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.0",
    "@types/module-alias": "^2.0.1",
